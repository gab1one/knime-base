{
  "elements": [
    {
      "type": "Section",
      "label": "Data",
      "elements": [
        {
          "type": "Control",
          "scope": "#/properties/model/properties/aggregationMethod",
          "options": {
            "format": "radio"
          }
        },
        {
          "type": "Control",
          "scope": "#/properties/model/properties/frequencyColumns",
          "options": {
            "format": "columnFilter"
          },
          "rule": {
            "effect": "DISABLE",
            "condition": {
              "scope": "#/properties/model/properties/aggregationMethod",
              "schema": {
                "const": "COUNT"
              }
            }
          }
        },
        {
          "type": "Control",
          "scope": "#/properties/model/properties/weightColumn",
          "options": {
            "format": "columnSelection",
            "showRowKeys": false,
            "showNoneColumn": true
          },
          "rule": {
            "effect": "DISABLE",
            "condition": {
              "scope": "#/properties/model/properties/aggregationMethod",
              "schema": {
                "enum": ["COUNT", "MIN", "MAX"]
              }
            }
          }
        },
        {
          "type": "Control",
          "scope": "#/properties/model/properties/categoryColumn",
          "options": {
            "format": "columnSelection",
            "showRowKeys": false,
            "showNoneColumn": true
          }
        }
      ]
    },
    {
      "type": "Section",
      "label": "Output",
      "elements": [
        {
          "type": "Control",
          "scope": "#/properties/model/properties/grandTotals",
          "options": {
            "format": "checkbox"
          },
          "rule": {
            "effect": "DISABLE",
            "condition": {
              "scope": "#/properties/model/properties/categoryColumn",
              "schema": {
                "const": "<none>"
              }
            }
          }
        }
      ]
    }
  ]
}