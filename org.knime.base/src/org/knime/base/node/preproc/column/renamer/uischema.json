{
    "elements": [
        {
            "type": "Control",
            "scope": "#/properties/model/properties/renamings",
            "options": {
                "addButtonText": "Add column",
                "detail": {
                    "renaming": {
                        "type": "HorizontalLayout",
                        "elements": [
                            {
                                "type": "Control",
                                "scope": "#/properties/oldName",
                                "options": {
                                    "format": "columnSelection",
                                    "showRowKeys": false,
                                    "showNoneColumn": false
                                }
                            },
                            {
                                "type": "Control",
                                "scope": "#/properties/newName"
                            }
                        ]
                    }
                }
            }
        }
    ]
}